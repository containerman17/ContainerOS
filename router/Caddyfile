{
    storage consul {
           address      "127.0.0.1:8500"
           timeout      10
           prefix       "caddytls"
           value_prefix "myprefix"
           tls_enabled  "false"
           tls_insecure "true"
    }

    cert_issuer zerossl

    supervisor {
        node /worker/worker.js {
            #dir /app/worker
            
            env APP_ENV production
                        
            restart_policy always 
                        
            replicas 1
        }
    }
}
